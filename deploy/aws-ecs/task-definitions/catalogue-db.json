{
    "family": "weavedemo-catalogue-db",
    "containerDefinitions": [
        {
            "essential": true,
            "image": "weaveworksdemos/catalogue-db",
            "cpu": 256,
            "memory": 256,
            "name": "catalogue-db",
            "environment": [
              {
                "name": "MYSQL_ROOT_PASSWORD",
                "value": "fake_password"
              },
              {
                "name": "MYSQL_DATABASE",
                "value": "socksdb"
              },
              {
                "name": "SERVICE_3306_NAME",
                "value": "catalogue-db"
              }
            ],
            "portMappings": [
              {
                "containerPort": 3306,
                "hostPort": 0,
                "protocol": ""
              }
            ]
        }
    ]
}
