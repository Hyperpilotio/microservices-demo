{
    "family": "weavedemo-edge-router",
    "containerDefinitions": [
        {
            "essential": true,
            "image": "weaveworksdemos/edge-router",
            "cpu": 256,
            "memory": 256,
            "name": "edge-router",
            "environment": [
              {
                "name": "SERVICE_80_NAME",
                "value": "edge-router"
              }
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
